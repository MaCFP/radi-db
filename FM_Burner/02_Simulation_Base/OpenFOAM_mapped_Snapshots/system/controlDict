    FoamFile
    {
        version     2.0;
        format      ascii;
        class       dictionary;
        location    "system";
        object      controlDict;
    }
    // * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
    
    application     simpleFoam;
    
    startTime 0;
    // startFrom       startTime;
    startFrom       latestTime;
    
    
    endTime         2000;
    // endTime 5.0;
    // stopAt          endTime;
    stopAt          writeNow;
    
    deltaT          1;
    
    writeControl    timeStep;
    
    writeInterval   1;
    
    purgeWrite      0;
    
    writeFormat     ascii;
    
    writePrecision  12;
    
    writeCompression on;
    
    timeFormat      general;
    
    timePrecision   12;
    
    runTimeModifiable true;

