FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startTime 0;
// startFrom       startTime;
startFrom       latestTime;


endTime         2000;
// endTime 5.0;
stopAt          endTime;
// stopAt          writeNow;

deltaT          0.0000001;

writeControl    timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression on;

timeFormat      general;

timePrecision   12;

runTimeModifiable true;

functions
{

    writeObjects1.writeObjects
    {
        type        writeObjects;
        libs        (utilityFunctionObjects);
        objects     (a);
        timeStart       0.0;
        timeEnd         1000;
        executeControl  timeStep;
        executeInterval 1;
        writeControl    timeStep;
        writeInterval   1;
        outputInterval 1;
    }

    wall_Line_x06_y0_z_0_83__cellPoint.sets {
        type sets;
        functionObjectLibs (“libsampling.so”);
        writeControl timeStep;
        interpolationScheme cellPoint;
        outputInterval 1; 
        writeInterval  1;
        setFormat   csv;
        sets (
            data
            {
                type uniform;
                axis z;
                start (0.6 0 0);
                end (0.6 0 0.83);
                nPoints 1000;
            } );
        fields (qin qr);
    }

    wall_Line_x06_y0_z_0_83__cellPatchConstrained.sets {
        type sets;
        functionObjectLibs (“libsampling.so”);
        writeControl timeStep;
        interpolationScheme cellPatchConstrained;
        outputInterval 1; 
        writeInterval  1;
        setFormat   csv;
        sets (
            data
            {
                type uniform;
                axis z;
                start (0.6 0 0);
                end (0.6 0 0.83);
                nPoints 1000;
            } );
        fields (qin qr);
    }

    wall_Line_x06_y0_z_0_83__cell.sets {
        type sets;
        functionObjectLibs (“libsampling.so”);
        writeControl timeStep;
        interpolationScheme cell;
        outputInterval 1; 
        writeInterval  1;
        setFormat   csv;
        sets (
            data
            {
                type uniform;
                axis z;
                start (0.6 0 0);
                end (0.6 0 0.83);
                nPoints 1000;
            } );
        fields (qin qr);
    }
    wall_Line_x06_y_n06_06_z_3__cellPatchConstrained.sets {
        type sets;
        functionObjectLibs (“libsampling.so”);
        writeControl timeStep;
        interpolationScheme cellPatchConstrained;
        outputInterval 1; 
        writeInterval  1;
        setFormat   csv;
        sets (
            data
            {
                type uniform;
                axis y;
                start (0.6 -0.6 3);
                end (0.6 0.6 0.3);
                nPoints 1000;
            } );
        fields (qin qr);
    }

    vertLine_x0_y_0_z_0_83__cell.sets {
        type sets;
        functionObjectLibs (“libsampling.so”);
        writeControl timeStep;
        interpolationScheme cell;
        outputInterval 1; 

        writeInterval  1;

        setFormat   csv;
        sets (
            data
            {
                type uniform;
                axis z;
                start (0.0 0 0);
                end (0.0 0 0.83);
                nPoints 1000;
            } );
        fields (a G T);
    }
}
